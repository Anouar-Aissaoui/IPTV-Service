{
  "rewrites": [
    {
      "source": "/((?!api|_next/static|_next/image|favicon.ico).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-DNS-Prefetch-Control",
          "value": "on"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(), interest-cohort=()"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' https:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https:; style-src 'self' 'unsafe-inline' https:; img-src 'self' data: https:; font-src 'self' https: data:; connect-src 'self' https:; media-src 'self' https:; object-src 'none'; frame-ancestors 'none';"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Resource-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    },
    {
      "source": "/(.*)\\.(jpg|jpeg|png|webp|avif|gif|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/en",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/en/(.*)",
      "destination": "/$1",
      "permanent": true
    },
    {
      "source": "/tutorials",
      "destination": "/iptv-setup-tutorials",
      "permanent": true
    },
    {
      "source": "/tutorials/smart-tv",
      "destination": "/iptv-setup-tutorials/how-to-setup-iptv-on-smart-tv",
      "permanent": true
    },
    {
      "source": "/tutorials/mobile",
      "destination": "/iptv-setup-tutorials/how-to-setup-iptv-on-mobile-devices",
      "permanent": true
    },
    {
      "source": "/tutorials/mag",
      "destination": "/iptv-setup-tutorials/how-to-setup-iptv-on-mag-box",
      "permanent": true
    },
    {
      "source": "/tutorials/formuler",
      "destination": "/iptv-setup-tutorials/how-to-setup-iptv-on-formuler-z8",
      "permanent": true
    },
    {
      "source": "/tutorials/fire-tv",
      "destination": "/iptv-setup-tutorials/how-to-setup-iptv-on-fire-tv-stick",
      "permanent": true
    },
    {
      "source": "/tutorials/enigma",
      "destination": "/iptv-setup-tutorials/how-to-setup-iptv-on-enigma2",
      "permanent": true
    },
    {
      "source": "/tutorials/vlc-windows",
      "destination": "/iptv-setup-tutorials/how-to-setup-iptv-on-vlc-windows",
      "permanent": true
    },
    {
      "source": "/premium-iptv-service",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/pricing",
      "destination": "/#pricing",
      "permanent": true
    },
    {
      "source": "/faq",
      "destination": "/#faq",
      "permanent": true
    }
  ]
}
